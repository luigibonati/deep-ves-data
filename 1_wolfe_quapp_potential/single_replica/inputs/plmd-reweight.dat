# vim:ft=plumed
UNITS NATURAL
p: READ FILE=COLVAR.3 VALUES=p.x IGNORE_TIME
nn: READ FILE=COLVAR.3 VALUES=nn.bias IGNORE_TIME
w: REWEIGHT_BIAS TEMP=1. ARG=nn.bias
h: HISTOGRAM ARG=p.x GRID_MIN=-3. GRID_MAX=3. GRID_BIN=100 BANDWIDTH=0.05 LOGWEIGHTS=w
fes: CONVERT_TO_FES GRID=h TEMP=1.
DUMPGRID GRID=fes FILE=reweight-500000.3 FMT=%24.16e
