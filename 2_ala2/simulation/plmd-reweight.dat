# vim:ft=plumed
phi: READ FILE=COLVAR.0-100000 VALUES=phi IGNORE_TIME
nn: READ FILE=COLVAR.0-100000 VALUES=nn.bias IGNORE_TIME
w: REWEIGHT_BIAS TEMP=300. ARG=nn.bias
h: HISTOGRAM ARG=phi GRID_MIN=-pi GRID_MAX=pi GRID_BIN=99 BANDWIDTH=0.05 LOGWEIGHTS=w
fes: CONVERT_TO_FES GRID=h TEMP=300.
DUMPGRID GRID=fes FILE=reweight-100000.0 FMT=%24.16e
